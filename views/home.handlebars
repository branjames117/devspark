<div id="containter">
    {{if starred repo}}
    <div>
        <a href={"https://github.com/users/" + user + "/" + repo.name}>
        <span>
            {{user}}/{{user.repo.name}}
        </span></a>
    </div>
    {{/if}}
    {{else}}
    <div>
        <p> This User has NO Starred Repos</p>
    </div>
</div>
<script src="../public/js/gitIssues.js"></script>
